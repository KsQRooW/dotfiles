{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os": "#ACB0BE",
    "closer": "p:os",
    "pink": "#F4B8E4",
    "lavender": "#BABBF1",
    "blue": "#8CAAEE"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:os",
          "style": "plain",
          "template": "\u256d\u2500\ueb99 ",
          "type": "os"
        },
        {
          "foreground": "p:blue",
          "style": "plain",
          "template": " {{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "p:pink",
          "properties": {
            "home_icon": "~",
            "style": "full"
          },
          "style": "plain",
          "template": "\ue5fe {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "p:lavender",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": true,
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }}{{ if .UpstreamIcon }} {{ .UpstreamIcon }} {{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "style": "plain",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "foreground": "p:closer",
          "template": "\u2570\u2500\uf105",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
