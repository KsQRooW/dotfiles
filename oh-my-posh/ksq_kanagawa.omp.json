{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "fuji_white": "#DCD7BA",
    "sakura_pink": "#D27E99",
    "spring_blue": "#7FB4CA",
    "wave_aqua": "#7AA89F",
    "carp_yellow": "#E6C384"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:fuji_white",
          "style": "plain",
          "template": "\u256d\u2500\ueb99 ",
          "type": "os"
        },
        {
          "foreground": "p:wave_aqua",
          "style": "plain",
          "template": " {{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "p:sakura_pink",
          "properties": {
            "home_icon": "~",
            "style": "full"
          },
          "style": "plain",
          "template": "\ue5fe {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "p:carp_yellow",
          "style": "plain",
          "template": "îˆµ {{ .Full }} ",
          "type": "python"
        },
        {
          "foreground": "p:spring_blue",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": true,
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }}{{ if .UpstreamIcon }} {{ .UpstreamIcon }} {{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "style": "plain",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "foreground": "p:fuji_white",
          "template": "\u2570\u2500\uf105",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
